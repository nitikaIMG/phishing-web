Options -Indexes
RewriteEngine On

# Force HTTPS
RewriteCond %{HTTPS} off
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

# Silent rewrite for /TechowlPhish2/Techowlphish/
RewriteCond %{REQUEST_URI} ^/$
RewriteRule ^(.*)$ /TechowlPhish2/Techowlphish/$1 [L]

# Redirect requests with .php extension to extension-less URL
# RewriteCond %{THE_REQUEST} \s/+(.*?/)?(.*?)\.php[\s?]
# RewriteRule ^ /%1%2 [R=301,L]

# Exclude CSS and JS files from rewrite rules
RewriteCond %{REQUEST_URI} !\.(css|js)$

# Remove .php extension and redirect to non-extension version
RewriteCond %{THE_REQUEST} \s/+(.*?/)?(.*?)\.php[\s?]
RewriteRule ^ /%1%2 [R=301,L]

# Handle non-extension URLs
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^([^/]+)/?$ $1.php [L]

# Handle multiple levels of inner pages with .php extension
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^(.+?)/([^/]+)/?$ $1/$2.php [L]

# Redirect requests with .php extension to extension-less URL
# RewriteCond %{THE_REQUEST} \s/+(.*?/)?(.*?)\.php[\s?]
# RewriteRule ^ /%1%2 [R=301,L]

